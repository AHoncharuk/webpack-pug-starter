{
	"name": "webpack-template",
	"version": "0.0.1",
	"description": "webpack-template",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"dev": "webpack-dev-server --config ./config/dev.cfg.js",
		"build": "rimraf ./dist && webpack --config ./config/prod.cfg.js --progress && size-limit",
		"lint": "eslint src/*.js",
		"lint-fix": "eslint --fix src/*.js",
		"server": "node ./config/server/server.js",
		"purify": "node ./config/helpers/purify.js",
		"shorty": "gulp shorty --gulpfile ./config/helpers/gulp-helpers.js",
		"iconfont": "rimraf src/icons/icon_font/icons/.gitkeep && svgo -f src/icons/icon_font/icons && node ./config/helpers/iconfont.js && rimraf src/fonts/**/*.{ttf,svg}",
		"icons": "gulp icons --gulpfile ./config/helpers/gulp-helpers.js && svgo -f src/icons/svg_inline",
		"clearfonts": "rimraf src/fonts/**/*.{css,html,eot,ttf,svg}",
		"size": "size-limit --why"
	},
	"size-limit": [
		{
			"path": "./dist/js/bundle.*.js"
		}
	],
	"devDependencies": {
		"autoprefixer": "^7.1.3",
		"babel-core": "^6.26.0",
		"babel-eslint": "^7.2.3",
		"babel-loader": "^7.1.2",
		"babel-preset-env": "^1.6.0",
		"babel-preset-es2015": "^6.24.1",
		"compression-webpack-plugin": "^1.0.0",
		"copy-webpack-plugin": "^4.0.1",
		"css-loader": "^0.28.5",
		"css-mqpacker": "^6.0.1",
		"eslint": "^4.6.1",
		"eslint-loader": "^1.9.0",
		"extract-text-webpack-plugin": "^3.0.0",
		"file-loader": "^0.11.2",
		"friendly-errors-webpack-plugin": "^1.6.1",
		"fs": "^0.0.1-security",
		"glob": "^7.1.2",
		"gulp": "^3.9.1",
		"gulp-cheerio": "^0.6.2",
		"gulp-newer": "^1.3.0",
		"gulp-shorthand": "^1.1.0",
		"gulp-svgmin": "^1.2.4",
		"html-webpack-plugin": "^2.30.1",
		"image-webpack-loader": "^3.4.2",
		"node-notifier": "^5.1.2",
		"node-sass": "^4.5.3",
		"node-static": "^0.7.9",
		"optimize-css-assets-webpack-plugin": "^3.1.1",
		"postcss": "^6.0.10",
		"postcss-browser-reporter": "^0.5.0",
		"postcss-loader": "^2.0.6",
		"progress-bar-webpack-plugin": "^1.10.0",
		"pug": "^2.0.0-rc.3",
		"pug-loader": "^2.3.0",
		"purify-css": "^1.2.5",
		"resolve-url-loader": "^2.1.0",
		"rimraf": "^2.6.1",
		"sass-loader": "^6.0.6",
		"size-limit": "^0.11.6",
		"style-loader": "^0.18.2",
		"stylelint": "^8.1.1",
		"svg-fill-loader": "^0.0.8",
		"svg-sprite-loader": "^3.2.5",
		"svgo": "^0.7.2",
		"svgo-loader": "^1.2.1",
		"uglifyjs-webpack-plugin": "^0.4.6",
		"underscore": "^1.8.3",
		"webfonts-generator": "^0.4.0",
		"webpack": "^3.5.5",
		"webpack-dev-server": "^2.7.1",
		"webpack-merge": "^4.1.0"
	},
	"dependencies": {
		"gsap": "^1.20.2",
		"jquery": "^3.2.1"
	}
}
